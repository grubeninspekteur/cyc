<!--
	AS ECLIPSE DOESN'T KNOW TEST / NON-TEST ENVIRONMENTS THIS FILE NEEDS TO BE RENAMED WHEN NEEDED
-->
<configuration debug="false">

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{35} - %msg %n
			</pattern>
		</encoder>
	</appender>

	<logger name="de.oglimmer.cyc" level="trace"></logger>

	<root level="error">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>